{
    "parser": "@typescript-eslint/parser",
    "extends": ["airbnb-base"],
    "plugins": ["@typescript-eslint"],
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [".ts"]
            }
        }
    },
    "rules": {
        "indent": ["error", 4],
        "no-trailing-spaces": "warn",
        "quotes": ["error", "double"],
        "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 1 }],
        "max-len": ["error", { "code": 125 }],
        "lines-between-class-members": "off",
        "no-plusplus": "off",
        "import/extensions": "off"
    }
}
